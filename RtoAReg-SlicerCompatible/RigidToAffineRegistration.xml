<?xml version="1.0" encoding="utf-8"?>
<executable>
  <category>
   BME186.FinalProject.EHAMMOND
  </category>
  <title>
   Rigid to Affine Registration
  </title>
  <description>
    A Slicer Module to perform a rigid to affine registration to initialize CT/MR pulmonary registration
  </description>
  <version>1.0</version>
  <documentation-url></documentation-url>
  <license></license>
  <contributor> Emily Hammond	Date: 5/1/2014 </contributor>
  <acknowledgements>This command module was derived from Insight/Examples/Filtering/ (copyright) Insight Software Consortium.  See http://wiki.na-mic.org/Wiki/index.php/Slicer3:Execution_Model_Documentation for more detailed descriptions.
  </acknowledgements>

  <parameters>
    <label>Fixed/Moving Volumes</label>
    <description>Selection of Fixed and Moving volumes</description>
    <image>
      <name>fixedVolume</name>
      <label>Fixed Volume</label>
      <channel>input</channel>
      <longflag>fixedVolume</longflag>
    </image>
    <image>
      <name>movingVolume</name>
      <label>Moving Volume</label>
      <channel>input</channel>
      <longflag>movingVolume</longflag>
    </image>
  </parameters>
  <parameters>
    <label>Output Volume</label>
    <description>Selection of Output Volume</description>
    <image>
      <name>outputVolume</name>
      <label>Output Volume</label>
      <channel>output</channel>
      <longflag>outputVolume</longflag>
    </image>
  </parameters>
  <parameters>
    <label>Mask Options</label>
    <description>Selection of Mask Volumes</description>
    <string-enumeration>
      <name>mask</name>
      <label>Mask</label>
      <longflag>mask</longflag>
      <default>no</default>
      <element>yes</element>
      <element>no</element>
    </string-enumeration>
    <image>
      <name>fixedMaskVolume</name>
      <label>Fixed Mask Volume</label>
      <channel>input</channel>
      <longflag>fixedMaskVolume</longflag>
    </image>
    <image>
      <name>movingMaskVolume</name>
      <label>Moving Mask Volume</label>
      <channel>input</channel>
      <longflag>movingMaskVolume</longflag>
    </image>
  </parameters>
</executable>
