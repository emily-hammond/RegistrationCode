<?xml version="1.0" encoding="utf-8"?>
<executable>
  <category>
   LongitudinalRegistration
  </category>
  <title>
   Label Map Overlap Module
  </title>
  <description>
    A Slicer Module to calculate the overlap between multiple labels in a label map with a variety of indices.
  </description>
  <version>1.0</version>
  <documentation-url></documentation-url>
  <license></license>
  <contributor> Emily Hammond	Date: 7/1/2015 </contributor>
  <acknowledgements>This command module was derived from Insight/Examples/Filtering/ (copyright) Insight Software Consortium.  See http://wiki.na-mic.org/Wiki/index.php/Slicer3:Execution_Model_Documentation for more detailed descriptions.
  </acknowledgements>

  <parameters>
    <label>Number of label maps</label>
    <description>Select the number of label maps for comparison</description>
	<integer>
	  <name>numberOfLM</name>
	  <label>Number of Label Maps</label>
	  <channel>input</channel>
	  <longflag>numberOfLM</longflag> 
	</integer>
  </parameters>
  <parameters>
    <label>Label maps</label>
    <description>Select each label map</description>
	<image type="label">
	  <name>labelMap1</name>
	  <label>Label Map 1</label>
	  <channel>input</channel>
	  <longflag>labelMap1</longflag>
	</image>
	<image type="label">
	  <name>labelMap2</name>
	  <label>Label Map 2</label>
	  <channel>input</channel>
	  <longflag>labelMap2</longflag>
	</image>
	<image type="label">
	  <name>labelMap3</name>
	  <label>Label Map 3</label>
	  <channel>input</channel>
	  <longflag>labelMap3</longflag>
	</image>
	<image type="label">
	  <name>labelMap4</name>
	  <label>Label Map 4</label>
	  <channel>input</channel>
	  <longflag>labelMap4</longflag>
	</image>
	<image type="label">
	  <name>labelMap5</name>
	  <label>Label Map 5</label>
	  <channel>input</channel>
	  <longflag>labelMap5</longflag>
	</image>
  </parameters>
  <parameters>
    <label>Desired index</label>
    <description>Select the indices to perform</description>
  </parameters>
</executable>
